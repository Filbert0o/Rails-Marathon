<h1> All the one singular house </h1>

  <p>NAME: <%= @house.name %></p>
  <p>AUTHOR: <%=@house.author%> </p>
  <p>MOTTO: <%= @house.motto %></p>
  <p>SOURCE: <%= @house.source %></p>

  <h3>Members</h3>

  <ul>
  <% @members.each do |member| %>
  <li>
    <%= member.first_name %><br>
    <%= member.last_name %><br>
    <%= member.created_at.strftime("%B %d %Y") %>
  </li>
  <% end %>
  </ul>
<%= link_to "Add a House Member", new_house_member_path(@house) %>
